create table public.radar_de_apalancamiento_signals (
  id bigint generated by default as identity not null,
  created_at timestamp with time zone not null default now(),
  bot_name text not null default 'Doublecuentas1.0'::text,
  is_safe_to_operate boolean not null,
  reason text null,
  last_pattern_found text null,
  losses_in_last_10_ops integer null,
  wins_in_last_5_ops integer null,
  historical_accuracy numeric(5, 2) null,
  pattern_found_at timestamp with time zone null,
  operations_after_pattern integer null default 0,
  auto_disable_after_ops integer null default 3,
  constraint radar_de_apalancamiento_signals_pkey primary key (id)
) TABLESPACE pg_default;
